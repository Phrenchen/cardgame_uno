  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "express": "^4.16.3",
    "mongoose": "^5.1.7",
    "nodemon": "^1.17.5",
    "object-keys": "^1.0.12",
    "uuid": "^3.3.2"
  },
  "devDependencies": {},
  "proxy": "http://localhost:5000"
}
