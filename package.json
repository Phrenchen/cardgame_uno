{
  "name": "traversy_mern_stack_tutorial",
  "version": "1.0.0",
  "engines": {
    "npm": "5.7.1"
  },
  "description": "shopping list build with MERN stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
